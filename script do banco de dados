CREATE SCHEMA `dbwgpark` ;

create table tbentrada(
  pkcodentr integer not null auto_increment,
  placacarentr varchar(10),
  dataentr varchar(10),
  horaentr varchar(10),
  primary key(pkcodentr));

create table tbsaida(
  pkcodsd integer not null auto_increment,
  placacarsd varchar(10),
  dataentrsd varchar(10),
  horaentrsd varchar(10),
  datasd varchar(10),
  horasd varchar(10),
  primary key (pkcodsd));

create table tbusuario(

  pkcodusuario integer not null auto_increment,
  nomeusuario varchar(20),
  senhausuario varchar(20),
  primary key (pkcodusuario));
